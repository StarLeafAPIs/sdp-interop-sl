{
  "name": "sdp-interop-sl",
  "description": "A simple SDP interoperability layer for Unified Plan/Plan B",
  "author": {"name": "Robert Taylor", "email": "rob.taylor@starleaf.com"},
  "contributors" : [
    {
      "name": "George Politis",
      "email": "gp@jitsi.org"
    } ],
  "version": "0.2.1",
  "stability": "unstable",
  "repository": {
    "type": "git",
    "url": "StarLeafRob/sdp-interop-sl"
  },
  "keywords": [
    "sdp",
    "webrtc",
    "interoperability",
    "polyfill"
  ],
